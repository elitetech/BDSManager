@page
@model IndexModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Dashboard";
}
@Html.AntiForgeryToken()
<div>
    <div class="dropdown-menu dropdown-menu-sm dropdown-menu-dark" id="server-context-menu">
        <button class="dropdown-item" id="context-server-start">Start</button>
        <button class="dropdown-item" id="context-server-stop">Stop</button>
        <button class="dropdown-item" id="context-server-restart">Restart</button>
        <hr class="dropdown-divider">
        <button class="dropdown-item" id="context-server-backup">Backup</button>
        <button class="dropdown-item" id="context-server-restore">Restore</button>
        <button class="dropdown-item" id="context-server-configure">Configure</button>
        <button class="dropdown-item btn-link btn-danger" id="context-server-remove">Remove</button>
    </div>
    <div class="dropdown-menu dropdown-menu-sm dropdown-menu-dark" id="player-context-menu">
        <div class="dropdown dropend">
            <button class="dropdown-item dropdown-toggle" aria-labelledby="player-context-whitelist-menu" data-bs-toggle="dropdown" aria-expanded="false" id="context-player-whitelist">Whitelist</button>
            <div class="dropdown-menu dropdown-menu-sm dropdown-menu-dark" id="player-context-whitelist-menu">
                <button class="dropdown-item" id="context-player-whitelist-add">Add</button>
                <button class="dropdown-item" id="context-player-whitelist-remove">Remove</button>
            </div>
        </div>
        <div class="dropdown dropend">
            <button class="dropdown-item dropdown-toggle" aria-labelledby="player-context-permissions-menu" data-bs-toggle="dropdown" aria-expanded="false" id="context-player-permissions">Permissions</button>
            <div class="dropdown-menu dropdown-menu-sm dropdown-menu-dark" id="player-context-permissions-menu">
                <button class="dropdown-item" id="context-player-permissions-member">Member</button>
                <button class="dropdown-item" id="context-player-permissions-operator">Operator</button>
            </div>
        </div>
        <hr class="dropdown-divider">
        <button class="dropdown-item" id="context-player-kick">Kick</button>
    </div>
    <h3 class="display-6 text-center">Dashboard</h3>
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-header">
                    <a asp-page="/ManageServer" asp-route-createNew=true class="btn btn-dark float-end">Add Server</a>
                    <h4>Servers</h4>
                </div>
                <div class="card-body server-list">
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Port</th>
                                <th>Version</th>
                                <th>Players</th>
                                <th>Online</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var server in Model.Servers)
                            {
                                var online = Model.ServerInstances.Any(x => x.Path == server.Path);
                                <tr data-server-path="@server.Path" class="server-row">
                                    <td><img class="server-icon" src="https://www.minecraft.net/etc.clientlibs/minecraft/clientlibs/main/resources/favicon.ico" /> @server.Options.Name</td>
                                    <td>@server.Options.Port</td>
                                    <td>@server.Version</td>
                                    <td id="server-player-count-@server.Path">@server.Players.Where(x => x.Online).Count() / @server.Options.MaxPlayers</td>
                                    <td id="server-status-@server.Path" class="server-status" data-server-uptime="@server.LastStarted">@(online ? $"Online" : "Offline")</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

    @foreach (var server in Model.Servers)
    {
        var instance = Model.ServerInstances.FirstOrDefault(x => x.Path == server.Path);
        var online = instance != null;
        <br />
        <div id="server-details-@server.Path" class="server-details" style="display: none;">
            <div class="row">
                <div class="col-md-4 col-sm-12 col-12" style="padding-bottom: 20px;">
                    <div class="card">
                        <div class="card-header">
                            <h5>@server.Options.Name</h5>
                        </div>
                        <div class="card-body player-list">
                            <div id="players-target-@server.Path">
                                <table class="table table-responsive table-striped">
                                    <thead>
                                        <tr>
                                            <th>Name</th>
                                            <th>Last Seen</th>
                                        </tr>
                                    </thead>
                                    <tbody id="server-player-list-@server.Path">
                                        @foreach (var player in server.Players)
                                        {
                                            var lastSeen = player.Online ? "Now" 
                                                : player.LastSeen?.ToString("MM/dd/yyyy") == DateTime.Now.ToString("MM/dd/yyyy") ? player.LastSeen?.ToString("hh:mm:ss tt") 
                                                : player.LastSeen?.ToString("MM/dd/yyyy hh:mm tt");
                                            <tr class="player-row" data-player-xuid="@player.XUID" data-player-name="@player.Name" data-server-path="@server.Path">
                                                <td>@player.Name</td>
                                                <td>@lastSeen</td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-8 col-sm-12 col-12">
                    <div class="card">
                        <div class="card-header">
                            <h5>Console</h5>
                        </div>
                        <div class="card-body">
                            <ul id="console-window-@server.Path" class="console-content list-group">
                                @{
                                    if (online && instance != null)
                                    {
                                        foreach (var logEntry in instance.ConsoleOutput)
                                        {
                                            var timestamp = "";
                                            var log = logEntry;
                                            if (logEntry.StartsWith("[") && logEntry.Contains("]")){
                                                timestamp = logEntry.Split(']')[0] + "]";
                                                log = logEntry.Replace(timestamp, "").Trim();
                                            }
                                            
                                            <li class="console-line list-group-item" data-bs-toggle="tooltip" data-bs-placement="top" title="@timestamp">
                                                <span class="console-line-content">@log</span>
                                            </li>
                                        }
                                    }
                                }
                            </ul>
                            <div class="input-group">
                                <div class="dropend">
                                    <button class="btn btn-secondary dropdown-toggle" 
                                        aria-labelledby="server-command-menu" 
                                        data-bs-toggle="dropdown" 
                                        aria-expanded="false" 
                                        id="server-command-menu-btn" 
                                        data-bs-auto-close="outside"
                                        disabled="@(!online)">/</button>
                                    <div class="dropdown-menu dropdown-menu-sm dropdown-menu-dark" id="server-command-menu">

                                        <!-- Give command -->
                                        <!-- give - target - item - amount -->
                                        <div class="dropend">
                                            <button class="dropdown-item dropdown-toggle" 
                                                aria-labelledby="server-command-give"
                                                data-bs-toggle="dropdown"
                                                aria-expanded="false"
                                                data-bs-auto-close="outside"
                                                id="server-command-give-btn">
                                                give
                                            </button>
                                            <div class="dropdown-menu dropdown-menu-sm dropdown-menu-dark" id="server-command-give">
                                                <div class="dropend">
                                                    <!-- get online users -->
                                                    @if(!server.Players.Any(x => x.Online)){
                                                        <li class="dropdown-item disabled">No players online</li>
                                                    }
                                                    @foreach (var player in server.Players)
                                                    {
                                                        <button class="dropdown-item dropdown-toggle" 
                                                            aria-labelledby="server-command-give-item"
                                                            data-bs-toggle="dropdown"
                                                            aria-expanded="false"
                                                            class="server-command-give-item-btn player-btn"
                                                            data-bs-auto-close="outside"
                                                            data-player-name="@player.Name"
                                                            disabled="@(!player.Online)">
                                                            @player.Name
                                                        </button>
                                                    }
                                                    <div class="dropdown-menu dropdown-menu-sm dropdown-menu-dark" id="server-command-give-items">
                                                        <div class="dropdown items-dropdown">
                                                            <!-- get items -->
                                                            <input type="text" class="form-control server-command-give-item-search" placeholder="Search" />
                                                            @foreach (var item in Model.Items)
                                                            {
                                                                <button class="dropdown-item dropdown-toggle"
                                                                    aria-labelledby="server-command-give-amount"
                                                                    data-bs-toggle="dropdown"
                                                                    aria-expanded="false"
                                                                    class="server-command-give-amount-btn"
                                                                    data-item-id-name="@item.IDName"
                                                                    >
                                                                    <img class="item-image" src="/images/@item.Image" />@item.Name
                                                                </button>
                                                            }
                                                            <div class="dropdown-menu dropdown-menu-sm dropdown-menu-dark" id="server-command-give-amounts">
                                                                <button class="dropdown-item" data-server-path="@server.Path" data-item-amount="1" onclick="giveCommand(this)">1</button>
                                                                <button class="dropdown-item" data-server-path="@server.Path" data-item-amount="16" onclick="giveCommand(this)">16</button>
                                                                <button class="dropdown-item" data-server-path="@server.Path" data-item-amount="64" onclick="giveCommand(this)">64</button>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- Give command end -->

                                        <!-- Teleport command -->
                                        <!-- tp - target - destination -->
                                        <div class="dropend">
                                            <button class="dropdown-item dropdown-toggle" 
                                                aria-labelledby="server-command-tp"
                                                data-bs-toggle="dropdown"
                                                aria-expanded="false"
                                                data-bs-auto-close="outside"
                                                id="server-command-tp-btn">
                                                teleport
                                            </button>
                                            <div class="dropdown-menu dropdown-menu-sm dropdown-menu-dark" id="server-command-tp">
                                                <div class="dropend">
                                                    <!-- get online users -->
                                                    @if(!server.Players.Any(x => x.Online)){
                                                        <li class="dropdown-item disabled">No players online</li>
                                                    }
                                                    @foreach (var player in server.Players)
                                                    {
                                                        <button class="dropdown-item dropdown-toggle" 
                                                            aria-labelledby="server-command-tp-player"
                                                            data-bs-toggle="dropdown"
                                                            aria-expanded="false"
                                                            class="server-command-tp-player-btn player-btn"
                                                            data-bs-auto-close="outside"
                                                            data-player-name="@player.Name"
                                                            disabled="@(!player.Online)">
                                                            @player.Name
                                                        </button>
                                                    }
                                                    <div class="dropdown-menu dropdown-menu-sm dropdown-menu-dark" id="server-command-tp-player">
                                                        <li class="dropdown-item">
                                                            <div class="input-group" style="min-width: 250px;">
                                                                <input type="text" class="form-control server-command-tp-player-x" placeholder="x" />
                                                                <input type="text" class="form-control server-command-tp-player-y" placeholder="y" />
                                                                <input type="text" class="form-control server-command-tp-player-z" placeholder="z" />
                                                                <button class="btn btn-outline-dark tp-btn" data-server-path="@server.Path" onclick="tpCommand(this)">go</button>
                                                            </div>
                                                        </li>
                                                        @foreach (var player in server.Players)
                                                        {
                                                            <button class="dropdown-item player-btn" 
                                                                data-server-path="@server.Path" 
                                                                onclick="tpCommand(this)"
                                                                data-player-name="@player.Name"
                                                                disabled="@(!player.Online)">
                                                                @player.Name
                                                            </button>
                                                        }

                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- Teleport command end -->

                                        <!-- Time command -->
                                        <div class="dropend">
                                            <button class="dropdown-item dropdown-toggle" 
                                                aria-labelledby="server-command-time"
                                                data-bs-toggle="dropdown"
                                                aria-expanded="false"
                                                data-bs-auto-close="outside"
                                                id="server-command-time-btn">
                                                time
                                            </button>
                                            <div class="dropdown-menu dropdown-menu-sm dropdown-menu-dark" id="server-command-time">
                                                <div class="dropend">
                                                    <button class="dropdown-item dropdown-toggle" 
                                                        aria-labelledby="server-command-time-set"
                                                        data-bs-toggle="dropdown"
                                                        aria-expanded="false"
                                                        data-bs-auto-close="outside"
                                                        id="server-command-time-set-btn">
                                                        set
                                                    </button>
                                                    <div class="dropdown-menu dropdown-menu-sm dropdown-menu-dark" id="server-command-time-set">
                                                        <li class="dropdown-item">
                                                            <div class="input-group" style="min-width: 250px;">
                                                                <input type="text" class="form-control server-command-time-set" placeholder="ticks" />
                                                                <button class="btn btn-outline-dark time-btn" data-server-path="@server.Path" onclick="timeSetCommand(this)">go</button>
                                                            </div>
                                                        </li>
                                                        <button class="dropdown-item" data-server-path="@server.Path" data-time="day" onclick="timeSetCommand(this)">day</button>
                                                        <button class="dropdown-item" data-server-path="@server.Path" data-time="night" onclick="timeSetCommand(this)">night</button>
                                                        <button class="dropdown-item" data-server-path="@server.Path" data-time="noon" onclick="timeSetCommand(this)">noon</button>
                                                        <button class="dropdown-item" data-server-path="@server.Path" data-time="midnight" onclick="timeSetCommand(this)">midnight</button>
                                                    </div>
                                                </div>
                                                <div class="dropend">
                                                    <button class="dropdown-item dropdown-toggle" 
                                                        aria-labelledby="server-command-time-add"
                                                        data-bs-toggle="dropdown"
                                                        aria-expanded="false"
                                                        data-bs-auto-close="outside"
                                                        id="server-command-time-add-btn">
                                                        add
                                                    </button>
                                                    <div class="dropdown-menu dropdown-menu-sm dropdown-menu-dark" id="server-command-time-add">
                                                        <li class="dropdown-item">
                                                            <div class="input-group" style="min-width: 250px;">
                                                                <input type="text" class="form-control server-command-time-add" placeholder="ticks" />
                                                                <button class="btn btn-outline-dark time-btn" data-server-path="@server.Path" onclick="timeAddCommand(this)">go</button>
                                                            </div>
                                                        </li>
                                                    </div>
                                                </div>
                                                <div class="dropend">
                                                    <button class="dropdown-item dropdown-toggle" 
                                                        aria-labelledby="server-command-time-query"
                                                        data-bs-toggle="dropdown"
                                                        aria-expanded="false"
                                                        data-bs-auto-close="outside"
                                                        id="server-command-time-query-btn">
                                                        query
                                                    </button>
                                                    <div class="dropdown-menu dropdown-menu-sm dropdown-menu-dark" id="server-command-time-query">
                                                        <button class="dropdown-item" data-server-path="@server.Path" data-query="daytime" onclick="timeQueryCommand(this)">daytime</button>
                                                        <button class="dropdown-item" data-server-path="@server.Path" data-query="day" onclick="timeQueryCommand(this)">day</button>
                                                        <button class="dropdown-item" data-server-path="@server.Path" data-query="gametime" onclick="timeQueryCommand(this)">gametime</button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- Time command end -->

                                        <!-- Weather command -->

                                        <div class="dropend">
                                            <button class="dropdown-item dropdown-toggle" 
                                                aria-labelledby="server-command-weather"
                                                data-bs-toggle="dropdown"
                                                aria-expanded="false"
                                                data-bs-auto-close="outside"
                                                id="server-command-weather-btn">
                                                weather
                                            </button>
                                            <div class="dropdown-menu dropdown-menu-sm dropdown-menu-dark" id="server-command-weather">
                                                <div class="dropend">
                                                    <button class="dropdown-item dropdown-toggle" 
                                                        aria-labelledby="server-command-weather-set"
                                                        data-bs-toggle="dropdown"
                                                        aria-expanded="false"
                                                        data-bs-auto-close="outside"
                                                        id="server-command-weather-set-btn">
                                                        set
                                                    </button>
                                                    <div class="dropdown-menu dropdown-menu-sm dropdown-menu-dark" id="server-command-weather-set">
                                                        <button class="dropdown-item" data-server-path="@server.Path" data-weather="clear" onclick="weatherSetCommand(this)">clear</button>
                                                        <button class="dropdown-item" data-server-path="@server.Path" data-weather="rain" onclick="weatherSetCommand(this)">rain</button>
                                                        <button class="dropdown-item" data-server-path="@server.Path" data-weather="thunder" onclick="weatherSetCommand(this)">thunder</button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- Weather command end -->

                                        <!-- Enchant command -->

                                        <div class="dropend">
                                            <button class="dropdown-item dropdown-toggle" 
                                                aria-labelledby="server-command-enchant"
                                                data-bs-toggle="dropdown"
                                                aria-expanded="false"
                                                data-bs-auto-close="outside"
                                                id="server-command-enchant-btn">
                                                enchant
                                            </button>
                                            <div class="dropdown-menu dropdown-menu-sm dropdown-menu-dark" id="server-command-enchant">
                                                <div class="dropend">
                                                    <!-- get online users -->
                                                    @if(!server.Players.Any(x => x.Online)){
                                                        <li class="dropdown-item disabled">No players online</li>
                                                    }
                                                    @foreach (var player in server.Players)
                                                    {
                                                        <button class="dropdown-item dropdown-toggle" 
                                                            aria-labelledby="server-command-enchant-player"
                                                            data-bs-toggle="dropdown"
                                                            aria-expanded="false"
                                                            class="server-command-enchant-player-btn player-btn"
                                                            data-bs-auto-close="outside"
                                                            data-player-name="@player.Name"
                                                            disabled="@(!player.Online)">
                                                            @player.Name
                                                        </button>
                                                    }

                                                    <div class="dropdown-menu dropdown-menu-sm dropdown-menu-dark" id="server-command-enchant-player">
                                                        <!-- mending -->
                                                        <div class="dropend">
                                                            <div class="dropdown-item dropdown-toggle" 
                                                                aria-labelledby="server-command-enchant-mending"
                                                                data-bs-toggle="dropdown"
                                                                aria-expanded="false"
                                                                data-bs-auto-close="outside"
                                                                id="server-command-enchant-mending-btn">
                                                                mending
                                                                <div class="dropdown-menu dropdown-menu-sm dropdown-menu-dark" id="server-command-enchant-mending">
                                                                    <li class="dropdown-item" data-server-path="@server.Path" data-enchantment="mending" data-enchatment-level="1" onclick="enchantPlayerCommand(this)">1</li>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        
                                                        <!-- unbreaking -->
                                                        <div class="dropend">
                                                            <div class="dropdown-item dropdown-toggle" 
                                                                aria-labelledby="server-command-enchant-unbreaking"
                                                                data-bs-toggle="dropdown"
                                                                aria-expanded="false"
                                                                data-bs-auto-close="outside"
                                                                id="server-command-enchant-unbreaking-btn">
                                                                unbreaking
                                                                <div class="dropdown-menu dropdown-menu-sm dropdown-menu-dark" id="server-command-enchant-unbreaking">
                                                                    <li class="dropdown-item" data-server-path="@server.Path" data-enchantment="unbreaking" data-enchatment-level="1" onclick="enchantPlayerCommand(this)">1</li>
                                                                    <li class="dropdown-item" data-server-path="@server.Path" data-enchantment="unbreaking" data-enchatment-level="2" onclick="enchantPlayerCommand(this)">2</li>
                                                                    <li class="dropdown-item" data-server-path="@server.Path" data-enchantment="unbreaking" data-enchatment-level="3" onclick="enchantPlayerCommand(this)">3</li>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        

                                                        <!-- general armor -->
                                                        <div class="dropend">
                                                            <div class="dropdown-item dropdown-toggle" 
                                                                aria-labelledby="server-command-enchant-armor"
                                                                data-bs-toggle="dropdown"
                                                                aria-expanded="false"
                                                                data-bs-auto-close="outside"
                                                                id="server-command-enchant-armor-btn">
                                                                armor
                                                                <div class="dropdown-menu dropdown-menu-sm dropdown-menu-dark" id="server-command-enchant-armor">
                                                                    <div class="dropend">
                                                                        <div class="dropdown-item dropdown-toggle"
                                                                            aria-labelledby="server-command-enchant-armor-enchantment"
                                                                            data-bs-toggle="dropdown"
                                                                            aria-expanded="false"
                                                                            data-bs-auto-close="outside"
                                                                            id="server-command-enchant-armor-enchantment-btn">
                                                                            blast protection
                                                                            <div class="dropdown-menu dropdown-menu-sm dropdown-menu-dark" id="server-command-enchant-armor-enchantment">
                                                                                <li class="dropdown-item" data-server-path="@server.Path" data-enchantment="blast_protection" data-enchatment-level="1" onclick="enchantPlayerCommand(this)">1</li>
                                                                                <li class="dropdown-item" data-server-path="@server.Path" data-enchantment="blast_protection" data-enchatment-level="2" onclick="enchantPlayerCommand(this)">2</li>
                                                                                <li class="dropdown-item" data-server-path="@server.Path" data-enchantment="blast_protection" data-enchatment-level="3" onclick="enchantPlayerCommand(this)">3</li>
                                                                                <li class="dropdown-item" data-server-path="@server.Path" data-enchantment="blast_protection" data-enchatment-level="4" onclick="enchantPlayerCommand(this)">4</li>
                                                                                <li class="dropdown-item" data-server-path="@server.Path" data-enchantment="blast_protection" data-enchatment-level="5" onclick="enchantPlayerCommand(this)">5</li>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                    <div class="dropend">
                                                                        <div class="dropdown-item dropdown-toggle"
                                                                            aria-labelledby="server-command-enchant-armor-enchantment"
                                                                            data-bs-toggle="dropdown"
                                                                            aria-expanded="false"
                                                                            data-bs-auto-close="outside"
                                                                            id="server-command-enchant-armor-enchantment-btn">
                                                                            fire protection
                                                                            <div class="dropdown-menu dropdown-menu-sm dropdown-menu-dark" id="server-command-enchant-armor-enchantment">
                                                                                <li class="dropdown-item" data-server-path="@server.Path" data-enchantment="fire_protection" data-enchatment-level="1" onclick="enchantPlayerCommand(this)">1</li>
                                                                                <li class="dropdown-item" data-server-path="@server.Path" data-enchantment="fire_protection" data-enchatment-level="2" onclick="enchantPlayerCommand(this)">2</li>
                                                                                <li class="dropdown-item" data-server-path="@server.Path" data-enchantment="fire_protection" data-enchatment-level="3" onclick="enchantPlayerCommand(this)">3</li>
                                                                                <li class="dropdown-item" data-server-path="@server.Path" data-enchantment="fire_protection" data-enchatment-level="4" onclick="enchantPlayerCommand(this)">4</li>
                                                                                <li class="dropdown-item" data-server-path="@server.Path" data-enchantment="fire_protection" data-enchatment-level="5" onclick="enchantPlayerCommand(this)">5</li>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                    <div class="dropend">
                                                                        <div class="dropdown-item dropdown-toggle"
                                                                            aria-labelledby="server-command-enchant-armor-enchantment"
                                                                            data-bs-toggle="dropdown"
                                                                            aria-expanded="false"
                                                                            data-bs-auto-close="outside"
                                                                            id="server-command-enchant-armor-enchantment-btn">
                                                                            projectile protection
                                                                            <div class="dropdown-menu dropdown-menu-sm dropdown-menu-dark" id="server-command-enchant-armor-enchantment">
                                                                                <li class="dropdown-item" data-server-path="@server.Path" data-enchantment="projectile_protection" data-enchatment-level="1" onclick="enchantPlayerCommand(this)">1</li>
                                                                                <li class="dropdown-item" data-server-path="@server.Path" data-enchantment="projectile_protection" data-enchatment-level="2" onclick="enchantPlayerCommand(this)">2</li>
                                                                                <li class="dropdown-item" data-server-path="@server.Path" data-enchantment="projectile_protection" data-enchatment-level="3" onclick="enchantPlayerCommand(this)">3</li>
                                                                                <li class="dropdown-item" data-server-path="@server.Path" data-enchantment="projectile_protection" data-enchatment-level="4" onclick="enchantPlayerCommand(this)">4</li>
                                                                                <li class="dropdown-item" data-server-path="@server.Path" data-enchantment="projectile_protection" data-enchatment-level="5" onclick="enchantPlayerCommand(this)">5</li>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                    <div class="dropend">
                                                                        <div class="dropdown-item dropdown-toggle"
                                                                            aria-labelledby="server-command-enchant-armor-enchantment"
                                                                            data-bs-toggle="dropdown"
                                                                            aria-expanded="false"
                                                                            data-bs-auto-close="outside"
